<!--feet flexion-->
 <joint name="left_foot_flex_joint" type="revolute">
	<origin xyz="0 0 0" rpy="0 0 0"/>
	<axis xyz= "0 1 0"/>
	<limit lower= "-0.785398163" upper= "1.57079633" effort = "2.645" velocity="6"/>
    <parent link="virtual_left_foot_2"/>
    <child link="left_foot"/>
